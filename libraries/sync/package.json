{
  "name": "@mp/sync",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./client": {
      "types": "./src/client.ts",
      "import": "./src/client.ts",
      "require": "./src/client.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts",
      "require": "./src/server.ts"
    },
    ".": {
      "types": "./src/shared.ts",
      "import": "./src/shared.ts",
      "require": "./src/shared.ts"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:inspect": "vitest --inspect-brk --no-file-parallelism"
  },
  "dependencies": {
    "@mp/std": "workspace:*",
    "@mp/ws": "workspace:*",
    "cbor-x": "1.6.0",
    "piscina": "5.0.0-alpha.0"
  },
  "devDependencies": {
    "@mp/tsconfig": "workspace:*",
    "type-fest": "4.35.0",
    "vitest": "2.0.3"
  }
}
