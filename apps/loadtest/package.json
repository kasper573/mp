  },
  "dependencies": {
    "@mp/auth": "workspace:*",
    "@mp/game": "workspace:*",
    "@mp/logger": "workspace:*",
    "@mp/rpc": "workspace:*",
    "@mp/server": "workspace:*",
    "@mp/std": "workspace:*",
    "@mp/time": "workspace:*",
    "@mp/ws": "workspace:*",
    "react": "catalog:"
  },
  "devDependencies": {
    "@mp/tsconfig": "workspace:*",
    "@types/yargs": "17.0.33",
    "@types/react": "catalog:",
    "@vanilla-extract/vite-plugin": "catalog:",
    "tsx": "catalog:",
    "vite": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "yargs": "17.7.2"
  }
}
