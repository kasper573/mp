    "@mp/event-router": "workspace:*",
    "@mp/game": "workspace:*",
    "@mp/gateway": "workspace:*",
    "@mp/logger": "workspace:*",
    "@mp/state": "workspace:*",
    "@mp/std": "workspace:*",
    "@mp/time": "workspace:*",
    "@mp/ws": "workspace:*"
  },
  "devDependencies": {
    "@mp/tsconfig": "workspace:*",
    "@preact/preset-vite": "catalog:",
    "@types/yargs": "17.0.33",
    "@vanilla-extract/vite-plugin": "catalog:",
    "tsx": "catalog:",
    "vite": "catalog:",
    "yargs": "17.7.2"
  }
}
