{
  "name": "@mp/area-server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/package.ts",
      "import": "./src/package.ts",
      "require": "./src/package.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsx esbuild.ts",
    "inspect": "pnpm devenv tsx --inspect src/main.ts",
    "inspect-brk": "pnpm devenv tsx --inspect-brk src/main.ts",
    "dev": "pnpm devenv tsx watch src/main.ts",
    "devenv": "dotenvx run -f ../../docker/.env.dev ../../docker/.env.shared -- pnpm"
  },
  "dependencies": {
    "@mp/auth": "workspace:*",
    "@mp/game": "workspace:*",
    "@mp/ioc": "workspace:*",
    "@mp/logger": "workspace:*",
    "@mp/rate-limiter": "workspace:*",
    "@mp/rpc": "workspace:*",
    "@mp/server-common": "workspace:*",
    "@mp/std": "workspace:*",
    "@mp/sync": "workspace:*",
    "@mp/telemetry": "workspace:*",
    "@mp/time": "workspace:*",
    "@mp/ws": "workspace:*",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2"
  },
  "devDependencies": {
    "@mp/tsconfig": "workspace:*",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "builtin-modules": "5.0.0",
    "esbuild": "0.25.6",
    "tsx": "catalog:"
  }
}
