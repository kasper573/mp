    "@vanilla-extract/vite-plugin": "npm:@vanilla-extract/vite-plugin4.0.15",
    "vite-plugin-top-level-await": "vite-plugin-top-level-await@no1.4.4",
    "vite-plugin-handlebars": "vite-plugin-handlebars@2.0.0",
    "vite-plugin-checker": "vite-plugin-checker@0.6.4",
    "vite-plugin-solid": "vite-plugin-solid@2.10.2",
    "vite-plugin-wasm": "vite-plugin-wasm@^3.3.0"
  }
