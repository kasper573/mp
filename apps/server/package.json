{
  "name": "@mp/server",
  "version": "0.0.0",
  "private": true,
  "types": "./src/package.ts",
  "exports": {
    ".": {
      "import": "./src/package.ts",
      "require": "./src/package.ts"
    }
  },
  "scripts": {
    "start": "NODE_ENV=production dotenv-flow tsx src/main.ts",
    "dev": "NODE_ENV=development dotenv-flow tsx watch src/main.ts --clear-screen=false"
  },
  "dependencies": {
    "@mp/events": "workspace:*",
    "@mp/excalibur": "workspace:*",
    "@mp/state": "workspace:*",
    "@mp/tse": "workspace:*",
    "express": "4.19.2",
    "cors": "2.8.5",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@mp/build": "workspace:*",
    "@mp/tsconfig": "workspace:*",
    "@types/express": "4.17.21",
    "@types/cors": "2.8.17",
    "dotenv-flow-cli": "1.1.1"
  }
}
