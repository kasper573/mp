{
  "name": "@mp/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts",
      "require": "./src/types.ts"
    }
  },
  "scripts": {
    "push": "typeorm schema:sync",
    "generate": "typeorm migration:generate",
    "migrate": "typeorm migration:run",
    "seed": "tsx seed.ts",
    "drop": "tsx drop.ts",
    "reset": "pnpm drop && pnpm push && pnpm seed",
    "devenv": "dotenvx run -f ../../docker/.env.dev ../../docker/.env.shared -- pnpm"
  },
  "dependencies": {
    "@mp/logger": "workspace:*",
    "@mp/math": "workspace:*",
    "@mp/oauth": "workspace:*",
    "@mp/std": "workspace:*",
    "pg": "8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@mp/tsconfig": "workspace:*",
    "@types/pg": "8.11.10"
  }
}
